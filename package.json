{
  "name": "blogy-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:types": "dotenv -e .env.local -- sh -c 'pnpx supabase gen types typescript --project-id $VITE_SUPABASE_PROJECT_ID > ./src/supabase/database.types.ts'",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest"
  },
  "dependencies": {
    "@fontsource-variable/noto-sans": "^5.0.5",
    "@supabase/supabase-js": "^2.45.3",
    "@tanstack/vue-query": "^5.54.2",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^10.11.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lucide-vue-next": "^0.408.0",
    "md-editor-v3": "^4.20.0",
    "msw": "^2.4.3",
    "pinia": "^2.2.2",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.3",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.14.2",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-vue": "^9.28.0",
    "i": "^0.3.7",
    "jsdom": "^24.1.3",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.4.5",
    "vite": "^5.4.3",
    "vite-plugin-vue-devtools": "^7.4.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.6"
  }
}
